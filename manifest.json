{
  "name": "拍卖助手",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "拍卖中,自动报价",
  "icons":{
    "16": "assistant.png",
    "48": "assistant.png",
    "128": "assistant.png"
  },
  "browser_action": {
    "default_title": "助手",
    "default_icon": "assistant.png",
    "default_popup": "assistant.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.4.1.min.js","contentScript.js"]
    }
  ],
  "permissions" : ["tabs", "activeTab"],
  "web_accessible_resources": ["inject.js"]
}